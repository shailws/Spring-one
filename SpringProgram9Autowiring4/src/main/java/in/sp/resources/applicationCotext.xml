<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean class="in.sp.beans.Address" id="addrId">
		<property name="houseNo" value="100"></property>
		<property name="city" value="Pune"></property>
		<property name="pinCode" value="123456"></property>
	</bean>

	<bean class="in.sp.beans.Student" id="stdId" >
		<property name="name" value="Shailesh"></property>
		<property name="rollNo" value="101"></property>
		<!-- <property name="address" ref="addrId"></property> -->
	</bean>

</beans>