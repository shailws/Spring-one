<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean class="in.sp.beans.Address" id="addrId">
		<constructor-arg value="222"></constructor-arg>
		<constructor-arg value="Mumbai"></constructor-arg>
		<constructor-arg value="123123"></constructor-arg>
	</bean>

	<bean class="in.sp.beans.Student" id="stdId">
		<constructor-arg value="102"></constructor-arg>
		<constructor-arg value="Abhi"></constructor-arg>
		<constructor-arg ref="addrId"></constructor-arg>
	</bean>

</beans>