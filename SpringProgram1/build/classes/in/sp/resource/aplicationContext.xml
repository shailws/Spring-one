	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean class="in.sp.beans.Student" id="stdId1">
		<property name="name" value="Shailesh"></property>
		<property name="rollno" value="101"></property>
		<property name="email" value="shailesh77@gmail.com"></property>
	</bean>
	
	<bean class="in.sp.beans.Student" id="stdId2">
		<property name="name" value="Abhi"></property>
		<property name="rollno" value="102"></property>
		<property name="email" value="abhi88@gmail.com"></property>
	</bean>

</beans>